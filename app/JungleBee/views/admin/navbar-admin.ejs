		<div class="navbar-collapse collapse">
			<ul class="nav navbar-nav  navbar-left">
				<li <% if (req.originalUrl == '/admin/dashboard') {%> class="active" <% } %> id="dashboard"><a href="/admin/dashboard">Dashboard</a></li>
